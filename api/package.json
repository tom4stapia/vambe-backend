{
  "name": "vambe-api",
  "private": true,
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "db:migrate": "sequelize-cli db:migrate",
    "db:seed": "ts-node node_modules/.bin/sequelize-cli db:seed:all",
    "db:seed:undo": "ts-node node_modules/.bin/sequelize-cli db:seed:undo:all",
    "db:rollback": "sequelize-cli db:migrate:undo",
    "db:drop": "sequelize-cli db:drop",
    "db:create": "sequelize-cli db:create",
    "db:reset": "sequelize-cli db:drop && sequelize-cli db:create && sequelize-cli db:migrate && npm run db:seed",
    "db:fresh": "sequelize-cli db:migrate:undo:all && sequelize-cli db:migrate && npm run db:seed",
    "build": "tsc"
  },
  "dependencies": {
    "celery-ts": "^1.2.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "ioredis": "^5.7.0",
    "morgan": "^1.10.0",
    "nodemon": "^3.1.9",
    "pg": "^8.13.1",
    "redis": "^5.8.2",
    "sequelize": "^6.37.5",
    "sequelize-cli": "^6.6.2"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.3.1",
    "@types/redis": "^4.0.10",
    "@types/sequelize": "^4.28.20",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
